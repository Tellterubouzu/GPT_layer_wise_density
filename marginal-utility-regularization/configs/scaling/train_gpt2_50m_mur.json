{
  "dataset_name": "HuggingFaceFW/fineweb-edu",
  "dataset_config": null,
  "text_column": "text",
  "train_split": "train",
  "eval_split": "validation",
  "block_size": 1024,
  "batch_size": 2,
  "eval_batch_size": 2,
  "learning_rate": 0.0003,
  "weight_decay": 0.1,
  "num_train_steps": 10000,
  "warmup_steps": 1000,
  "eval_steps": 1000,
  "save_steps": 10000,
  "log_steps": 50,
  "gradient_accumulation_steps": 1,
  "max_grad_norm": 1.0,
  "seed": 42,
  "tokenizer_name": "gpt2",
  "run_name": "auto",
